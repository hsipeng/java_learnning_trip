package jdbc;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.Statement;
/***
 * SELECT *　
 * FROM
 * (SELECT ROWNUM rn, t.* 
 * FROM
 * (SELECT empno,ename,sal,job,deptno
 * FROM emp
 * ORDER BY sal DESC) t 
 * WHERE ROWNUM <= x)
 * WHERE RN  >=5
 * @author admin
 *
 */
public class JDBCDemo7 {
	public static void main(String[] args) {
		try {
			Class.forName("orcale.jdbc.driver.OracleDriver");
			Connection conn = DriverManager.getConnection("jdbc:oracle:thin:@localhost:1521:xe","system","Or175824");
			System.out.println("链接成功");
			Statement state = conn.createStatement();
			String sql = "SELECT * "
					+ "FROM  (SELECT ROWNUM rn, t.* "
					+ "FROM (SELECT empno,ename,sal,job,deptno "
					+ "FROM emp ORDER BY sal DESC) t "
					+ "WHERE ROWNUM <= x) "
					+ "WHERE RN  >=5";
			System.out.println(sql);
			
			
			
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
