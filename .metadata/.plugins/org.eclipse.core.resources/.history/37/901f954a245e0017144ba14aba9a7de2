<script type="text/javascript">
Window.onload=function(){
	var clist ='<%=session.getAttribute("clist") %>';
	var commentslist = document.getElementById("commentsList");
	var p="";
	for(var c in clist){
		p+="<p>"+c.getContent()+"<p>";
	}
	commentslist.innerHTML=p;
}
</script>
